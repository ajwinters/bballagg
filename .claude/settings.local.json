{
  "permissions": {
    "allow": [
      "Bash(powershell -c \"[console]::beep\\(800,200\\); [console]::beep\\(1000,300\\)\")",
      "Bash(.venv/Scripts/python.exe:*)"
    ]
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "powershell -c \"[console]::beep(600,150); [console]::beep(800,150); [console]::beep(1000,300)\""
          }
        ]
      }
    ]
  }
}
